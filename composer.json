{
  "name":         "specky-rum/zxcvbn-php",
  "type":         "library",
  "description":  "Realistic password strength estimation PHP library based on Zxcvbn JS",
  "keywords":     [
    "zxcvbn",
    "password"
  ],
  "homepage":     "https://github.com/specky-rum/zxcvbn-php",
  "license":      "MIT",
  "authors":      [
    {
      "name":     "See contributors",
      "homepage": "https://github.com/specky-rum/zxcvbn-php"
    }
  ],
  "require":      {
    "php":                       "^5.6 || ^7.0 || ^8.0",
    "symfony/polyfill-mbstring": ">=1.3.1"
  },
  "require-dev":  {
    "phpunit/phpunit":             "< 6.0",
    "php-coveralls/php-coveralls": "*",
    "squizlabs/php_codesniffer":   "3.*"
  },
  "autoload":     {
    "psr-4": {
      "ZxcvbnPhp\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ZxcvbnPhp\\Test\\": "test/"
    }
  }
}
